{
  "name": "Veue Streamer",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "This extension allows you to become a streamer on Veue!",
  "homepage_url": "https://veuelive.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "contentSettings",
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*/*",
        "http://*/*"
      ],
      "all_frames": true,
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}